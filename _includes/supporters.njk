<div class="lo lo-separator-h supporters-gold" style="--lo-margin-h: 2em">
	<div class="lo-c">
		<ul class="inlinelist list-superfeatured">
		{% for supporter in opencollective.supporters %}{% if supporter.tier == "Exclusive Gold Sponsor" -%}
		{%- set nameToSlug = supporter.name | slug | lower -%}
			<li class="inlinelist-item"><a href="{{ supporter.website or supporter.profile }}" class="elv-externalexempt" data-supporters-slug="{{ nameToSlug }}" rel="nofollow">{% avatarlocalcache "opencollective", nameToSlug %}{{ supporter.name }}</a></li>
		{%- endif %}{% endfor %}
		</ul>
	</div>
	<div class="lo-c lo-maxgrow lo-nocontentwrap lo-c-center">
		<h3 class="subhed">Exclusive <strong>Gold</strong> Sponsor</h3>
	</div>
</div>

<!-- TODO some kind of aspect ratio? -->
<div data-import="/imports/supporters.html"></div>