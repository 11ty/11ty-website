<div class="lo lo-separator-h supporters-gold" style="--lo-margin-h: 2em">
	<div class="lo-c">
		<ul class="inlinelist list-superfeatured">
		{% for supporter in supporters %}{% if supporter.tier == "Exclusive Gold Sponsor" -%}
		{%- set nameToSlug = supporter.name | slug | lower -%}
			<li class="inlinelist-item"><a href="{{ supporter.website or supporter.profile }}" class="elv-externalexempt" data-supporters-slug="{{ nameToSlug }}">{% avatarlocalcache "opencollective", nameToSlug, supporter.name %}{{ supporter.name }}</a></li>
		{%- endif %}{% endfor %}
		</ul>
	</div>
	<div class="lo-c lo-maxgrow lo-nocontentwrap lo-c-align-v-center">
		<h3 class="subhed">Exclusive <strong>Gold</strong> Sponsor</h3>
	</div>
</div>

<div class="facepile facepile-supporters">
{% for supporter in supporters | supportersFacepile %}
{%- if loop.first %}<div class="facepile-block">{% endif %}
{%- set nameToSlug = supporter.name | slug | lower -%}
	<a href="{{ supporter.website or supporter.profile }}" class="elv-externalexempt" data-supporters-slug="{{ nameToSlug }}">{% avatarlocalcache "opencollective", nameToSlug, supporter.name %}</a>
{%- if loop.last or (loop.index0 % 60 == 0 and not loop.first) %}</div>{% endif %}
{%- if not loop.first and loop.index0 % 60 == 0 and not loop.last %}<div class="facepile-block">{% endif %}
{% endfor %}
{%- if config.localDev -%}
	<a href="https://zachleat.com/" class="elv-externalexempt" data-supporters-slug="zach-leatherman"><img src="/img/avatar-local-cache/opencollective/zach-leatherman.jpg" alt="Zach Leatherman" loading="lazy" class="avatar"></a>
{%- endif -%}
</div>