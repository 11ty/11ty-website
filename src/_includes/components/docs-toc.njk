<div class="elv-toc">

<details open>
	<summary class="btn-primary benchnine rainbow-active rainbow-active-noanim">Menu</summary>
	{%- if eleventyNavigation -%}
		{%- set navKey = eleventyNavigation.key -%}
	{%- endif -%}
	{{ collections.sidebarNav | eleventyNavigation | eleventyNavigationToHtml({ activeKey: navKey, listClass: 'elv-toc-list', activeListItemClass: 'elv-toc-active', listItemHasChildrenClass: 'elv-toc-has-children' }) | safe }}
</details>

</div>
