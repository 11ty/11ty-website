<ul class="list-bare mb-2 fl fl-inline fl-nowrap">
	<li><em><i class="fa-solid fa-hammer"></i>Built with Eleventy v{{ eleventy.version }}</em></li>
	<li><a href="https://fontawesome.com/"><i class="fa-brands fa-font-awesome"></i>Font Awesome</a></li>
	<li>{% include "edit-on-github.njk" %}</li>
	<li><a href="/docs/accessibility/"><i class="fa-solid fa-universal-access"></i>Accessibility</a></li>
	<li><a href="https://zachleat.com" class="elv-externalexempt">{% communityavatar "zachleat" %}</a></li>
</ul>
<ul class="list-bare mb-2 fl fl-inline fl-nowrap">
	<li><a href="/docs/credits/"><i class="fa-solid fa-handshake-angle"></i>Credits</a></li>
	<li><a href="/firehose/"><i class="fa-solid fa-fire"></i>Firehose</a></li>
	<li><a href="/styleguide/"><i class="fa-solid fa-border-top-left"></i>Style Guide</a></li>
{% if github and github.stargazers %}
	<li><a href="https://github.com/11ty/eleventy/" class="elv-externalexempt" title="{{ github.stargazers }}"><i class="fa-solid fa-star"></i>{{ github.stargazers | humanReadableNum }} Stars</a></li>
{% endif %}
{% if npm and npm.downloads %}
	<li><i class="fa-solid fa-download"></i><abbr title="{{ npm.downloads }}">{{ npm.downloads | humanReadableNum }}</abbr> Downloads</li>
{% endif %}
{% if opencollective and opencollective.backers %}
	<li><a href="/docs/supporters/"><i class="fa-solid fa-hand-holding-medical"></i>{{ opencollective.backers | humanReadableNum }} Backers</a></li>
{% endif %}
</ul>
