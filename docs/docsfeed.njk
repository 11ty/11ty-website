---
layout: false
permalink: /docs/feed.xml
metadata:
  title: Eleventy Documentation
  url: https://www.11ty.dev/
  author:
    name: Zach Leatherman
    email: zach@zachleat.com
  feed:
    subtitle: Full Eleventy Documentation in feed form, sorted alphabetically.
    filename: feed.xml
    path: docs/feed.xml
    url: https://www.11ty.dev/docs/feed.xml
    id: https://www.11ty.dev/
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ metadata.title }}</title>
  <subtitle>{{ metadata.feed.subtitle }}</subtitle>
  <link href="{{ metadata.feed.url }}" rel="self"/>
  <link href="{{ metadata.url }}"/>
  <updated>{{ collections.docs | rssLastUpdatedDate }}</updated>
  <id>{{ metadata.feed.id }}</id>
  <author>
    <name>{{ metadata.author.name }}</name>
    <email>{{ metadata.author.email }}</email>
  </author>
  {%- for post in collections.docs %}
  {% set absolutePostUrl %}{{ post.url | url | absoluteUrl(metadata.url) }}{% endset %}
  <entry>
    <title>{{ post.fileSlug | replace("-", " ") | capitalize }}</title>
    <link href="{{ absolutePostUrl }}"/>
    <updated>{{ post.date | rssDate }}</updated>
    <id>{{ absolutePostUrl }}</id>
    <content type="html">{{ post.templateContent | htmlToAbsoluteUrls(absolutePostUrl) }}</content>
  </entry>
  {%- endfor %}
</feed>